{
  "apiVersion": "grizzly.grafana.com/v1alpha1",
  "kind": "GrafanaRuleGroup",
  "metadata": {
    "folder": "Azure Data Explorer",
    "name": "rulegroup"
  },
  "spec": {
    "interval": "1m",
    "rules": [
      {
        "expr": "",
        "for": "5m",
        "grafana_alert": {
          "title": "fff",
          "condition": "B",
          "data": [
            {
              "refId": "A",
              "queryType": "",
              "relativeTimeRange": {
                "from": 600,
                "to": 0
              },
              "datasourceUid": "__DATASOURCE__",
              "model": {
                "hide": false,
                "intervalMs": 1000,
                "maxDataPoints": 43200,
                "refId": "A"
              }
            },
            {
              "refId": "B",
              "queryType": "",
              "relativeTimeRange": {
                "from": 0,
                "to": 0
              },
              "datasourceUid": "-100",
              "model": {
                "conditions": [
                  {
                    "evaluator": {
                      "params": [
                        3
                      ],
                      "type": "gt"
                    },
                    "operator": {
                      "type": "and"
                    },
                    "query": {
                      "params": [
                        "A"
                      ]
                    },
                    "reducer": {
                      "params": [],
                      "type": "last"
                    },
                    "type": "query"
                  }
                ],
                "datasource": {
                  "type": "__expr__",
                  "uid": "-100"
                },
                "hide": false,
                "intervalMs": 1000,
                "maxDataPoints": 43200,
                "refId": "B",
                "type": "classic_conditions"
              }
            }
          ],
          "intervalSeconds": 60,
          "rule_group": "MyRuleGroup",
          "no_data_state": "NoData",
          "exec_err_state": "Alerting"
        }
      }
    ]
  }
}